	-@rm -f *.o ${srcdir}/*.o atomic_fai atomic
	-@rm -f atomic_fai.exe atomic.exe
	-@rm -f core core.[0-9]*
	-@rm -f *.lo ${srcdir}/*.lo
	-@rm -f *.irlog*
	(cd ch3 && ${MAKE} clean )
